uniform sampler2D texture;
uniform lowp float gray;
varying highp vec2 v_texCoord;
void main ()
{
  lowp vec4 frag_1;
  lowp vec4 tmpvar_2;
  tmpvar_2 = texture2D (texture, v_texCoord);
  frag_1.w = tmpvar_2.w;
  frag_1.xyz = mix (tmpvar_2.xyz, vec3(dot (tmpvar_2.xyz, vec3(0.299, 0.587, 0.114))), gray);
  gl_FragColor = frag_1;
}

